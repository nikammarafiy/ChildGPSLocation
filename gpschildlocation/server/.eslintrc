{
    "env": {
        "node": true
    },

    "rules": {
        "valid-jsdoc": 2,
        "no-eq-null": 2,
        "no-self-compare": 2,
        "no-void": 2,
        "no-warning-comments": 1,
        "brace-style": 2,
        "comma-style": 2,
        "no-multiple-empty-lines": 2,
        "no-lonely-if": 2,
        "no-nested-ternary": 2,
        "quotes": [2, "single"],
        "space-after-keywords": 2,
        "space-before-blocks": 2,
        "space-in-parens": 2,
        "spaced-comment": 2,
        "no-underscore-dangle": 0,
        "no-warning-comments": [0, { "terms": ["todo"], "location": "start" }]
    }
}
